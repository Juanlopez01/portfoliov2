<style>
  .slider {
    background-color: #1c1c37;
    width: 99.9vw;
    height: auto;
    margin: auto;
    overflow: hidden;
    color: #cdc6bb;
  }

  .slider .slide-track {
    display: flex;
    animation: scroll 50s linear infinite;
    -webkit-animation: scroll 40s linear infinite;
    width: calc(300px * 6);
  }
  .slider .slide {
    width: 300px;
  }

  .slider .slide p {
    width: 100%;
  }

  @keyframes scroll {
    0% {
      -webkit-transform: translateX(0);
      transform: translateX(0);
    }
    100% {
      -webkit-transform: translateX(calc(-100px * 3));
      transform: translateX(calc(-100px * 3));
    }
  }
</style>
